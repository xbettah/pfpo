	jar cvfe ./jar/Agenda.jar fp2.poo.principal.Principal \
	-C ./bin fp2/poo/pfpooadrdelvil1/CorreoElectronico.class \
	-C ./bin fp2/poo/pfpooadrdelvil1/Domicilio.class \
	-C ./bin fp2/poo/pfpooadrdelvil1/Persona.class \
	-C ./bin fp2/poo/pfpooadrdelvil1/Telefono.class \
	-C ./bin fp2/poo/pfpooadrdelvil1/OrdenacionNombre.class \
	-C ./bin fp2/poo/pfpooadrdelvil1/OrdenacionCorreo.class \
	-C ./bin fp2/poo/pfpooadrdelvil1/Contacto.class \
	-C ./bin fp2/poo/pfpooadrdelvil1/Agenda.class \
	-C ./bin fp2/poo/principal/Principal.class \
	-C ./bin fp2/poo/utilidades/ContactoInterfaz.class \
	-C ./bin fp2/poo/utilidades/CorreoElectronicoInterfaz.class \
	-C ./bin fp2/poo/utilidades/DomicilioInterfaz.class \
	-C ./bin fp2/poo/utilidades/PersonaInterfaz.class \
	-C ./bin fp2/poo/utilidades/TelefonoInterfaz.class \
